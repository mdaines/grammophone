digraph "<%- title %>" {

  graph [rankdir=LR];
  node [shape=record];

  <% automaton.forEach(function(state, index) { %>
    s<%- index %> [label="<%- index %> | <%- state.items.map(function(item) { return Helpers.bareFormatItem(item, start, productions, info); }).join("\\n") %>"];
  <% }); %>

  <% automaton.forEach(function(state, index) { %>
    <% var s; %>
    <% for (s in state.transitions) { %>
      s<%- index %> -> s<%- state.transitions[s] %> [label="<%- Helpers.escapeHTML(s) %>"];
    <% } %>
  <% }); %>

}
